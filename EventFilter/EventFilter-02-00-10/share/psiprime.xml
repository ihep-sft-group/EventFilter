<?xml version="1.0" encoding="UTF-8" standalone="no" ?>

<!DOCTYPE SIGNATURE_LIST SYSTEM "defaultsiglist.dtd">

<SIGNATURE_LIST>

  <SIGNATURE signature_id="HIT" prescale="1" forced_accept="2">
    <CRITERIA behaviour="accept" mark="GJunk">
      <ITEM name="nmdc" value="5000" type="MIN" />
      <ITEM name="nemc" value="100" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Junk">
      <ITEM name="nmdc" value="5000" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GJunk">
      <ITEM name="nemc" value="800" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Junk">
      <ITEM name="nemc" value="500" type="MIN" />
    </CRITERIA>
    <!--CRITERIA behaviour="accept" mark="GBBhabha">
      <ITEM name="rndm" value="0.3" type="MAX" />
    </CRITERIA-->
    <!--CRITERIA behaviour="jump" next="MDC">
      <ITEM name="nmuc" value="8" type="MIN" />
    </CRITERIA-->
  </SIGNATURE>

  <SIGNATURE signature_id="EMC" prescale="1" forced_accept="2">
    <CRITERIA behaviour="jump" next="MDC">
      <ITEM name="etot" value="1" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GHadron">
      <ITEM name="ebal" value="0.6" type="MAX" />	
      <ITEM name="nmuc" value="5" type="MAX" />
      <ITEM name="etot" value="2.8" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Hadron">
      <ITEM name="etot" value="2.8" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Error">
      <ITEM name="etot" value="5" type="MIN" />
    </CRITERIA>
  </SIGNATURE>

  <SIGNATURE signature_id="CST" prescale="1" forced_accept="2">
    <CRITERIA behaviour="continue" mark="LUM">
      <ITEM name="eend" value="2.8" type="MIN" />	
      <ITEM name="etot" value="6" type="MAX" /> 
      <ITEM name="acop" value="23" type="MAX" />
      <!--ITEM name="acole" value="40" type="MAX" /-->
      <ITEM name="emax1" value="1.4" type="MIN" />
      <ITEM name="emax2" value="0.8" type="MIN" />	
      <!--ITEM name="ebal" value="0.6" type="MAX" /-->
      <ITEM name="coste1" value="0.84" type="MIN" />
      <ITEM name="coste2" value="-0.84" type="MAX" />   	
    </CRITERIA>
    <CRITERIA behaviour="continue" mark="LUM">
      <ITEM name="eend" value="2.8" type="MIN" />
      <ITEM name="etot" value="6" type="MAX" />
      <ITEM name="acop" value="23" type="MAX" />
      <!--ITEM name="acole" value="40" type="MAX" /-->
      <ITEM name="emax1" value="1.4" type="MIN" />
      <ITEM name="emax2" value="0.8" type="MIN" />
      <!--ITEM name="ebal" value="0.6" type="MAX" /-->
      <ITEM name="coste1" value="-0.84" type="MAX" />
      <ITEM name="coste2" value="0.84" type="MIN" />   
    </CRITERIA>
    <CRITERIA behaviour="jump" next="SEC">
      <ITEM name="acop" value="23" type="MAX" />
      <ITEM name="emax1" value="1.4" type="MIN" />
      <ITEM name="emax2" value="0.8" type="MIN" />    
      <!--ITEM name="ebal" value="0.7" type="MAX" /-->     
    </CRITERIA>	
    <CRITERIA behaviour="accept" mark="Hadron">
      <ITEM name="etot" value="5" type="MAX" /> 
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Error">
    </CRITERIA>
  </SIGNATURE>

  <SIGNATURE signature_id="SEC" prescale="1" forced_accept="2">
    <CRITERIA behaviour="accept" mark="GDiphoton">
      <ITEM name="acop" value="3" type="MAX" />	
      <ITEM name="ebar" value="2.8" type="MIN" /> 
      <ITEM name="nsec" value="10" type="MAX" />
      <ITEM name="acole" value="40" type="MAX" />
      <ITEM name="coste1" value="0.81" type="MAX" />
      <ITEM name="coste2" value="0.81" type="MAX" />
      <ITEM name="coste1" value="-0.81" type="MIN" />
      <ITEM name="coste2" value="-0.81" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Diphoton">
      <ITEM name="acop" value="5" type="MAX" /> 
      <ITEM name="nsec" value="10" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GEBhabha">
      <ITEM name="acop" value="7" type="MIN" />
      <ITEM name="acole" value="40" type="MAX" />	
      <ITEM name="eend" value="2.8" type="MIN" />
      <ITEM name="nsec" value="30" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GBBhabha">
      <ITEM name="acop" value="6" type="MIN" />
      <ITEM name="acole" value="40" type="MAX" />
      <ITEM name="coste1" value="0.81" type="MAX" />
      <ITEM name="coste2" value="0.81" type="MAX" />
      <ITEM name="coste1" value="-0.81" type="MIN" />
      <ITEM name="coste2" value="-0.81" type="MIN" />	
      <ITEM name="ebar" value="2.8" type="MIN" />
      <ITEM name="nsec" value="35" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Hadron">
    </CRITERIA>
  </SIGNATURE>

  <SIGNATURE signature_id="MDC" prescale="1" forced_accept="2">
    <CRITERIA behaviour="jump" next="TOF">
      <ITEM name="ntrk" value="0" type="TRUE" />
      <ITEM name="nmuc" value="8" type="MAX" />
      <ITEM name="ebal" value="0.5" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GCosmic">
      <ITEM name="ntrk" value="0" type="TRUE" />
      <ITEM name="nmuc" value="8" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Cosmic">
      <ITEM name="ntrk" value="0" type="TRUE" />
      <ITEM name="nmuc" value="3" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Hadron">
      <ITEM name="ntrk" value="1" type="TRUE" />
      <ITEM name="vz" value="50" type="MAX" />
      <ITEM name="vz" value="-50" type="MIN" />
      <ITEM name="vr" value="1.5" type="MAX" />
      <ITEM name="vr" value="-1.5" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GBeamgas">
      <ITEM name="ntrk" value="1" type="TRUE" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GBeamgas">
      <ITEM name="ntrk" value="0" type="TRUE" />
      <ITEM name="ebal" value="0.5" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Hadron">
      <ITEM name="ntrk" value="0" type="TRUE" />
      <ITEM name="etot" value="0.1" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Beamgas">
      <ITEM name="ntrk" value="0" type="TRUE" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Hadron">
      <ITEM name="ntrk" value="2" type="FALSE" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Hadron">
      <ITEM name="acol" value="10" type="MIN" />
    </CRITERIA>
  </SIGNATURE>
 
  <SIGNATURE signature_id="TOF" prescale="1" forced_accept="2">
    <CRITERIA behaviour="accept" mark="GDimuon">
      <ITEM name="nmuc" value="10" type="MIN" />
      <ITEM name="ntrk" value="2" type="TRUE" />
      <ITEM name="dtof" value="-3" type="MIN" />
      <ITEM name="ntof" value="2" type="MIN" />
      <ITEM name="dtof" value="3" type="MAX" />
      <ITEM name="vz" value="50" type="MAX" />
      <ITEM name="vz" value="-50" type="MIN" />
      <ITEM name="vr" value="1.5" type="MAX" />
      <ITEM name="vr" value="-1.5" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Dimuon">
      <ITEM name="ntrk" value="2" type="TRUE" />
      <ITEM name="dtof" value="-5" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GBeamgas">
      <ITEM name="ntrk" value="0" type="TRUE" />
      <ITEM name="dtof" value="-5" type="MIN" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Cosmic">
      <ITEM name="ntrk" value="0" type="TRUE" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="GCosmic">
      <ITEM name="ntrk" value="2" type="TRUE" />
      <ITEM name="dtof" value="-5" type="MAX" />
    </CRITERIA>
    <CRITERIA behaviour="accept" mark="Cosmic">
    </CRITERIA>
  </SIGNATURE>

</SIGNATURE_LIST>
